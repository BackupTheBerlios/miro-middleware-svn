# common build rules

.xml.cpp:
	$(top_builddir)/src/makeParams/MakeParams -f $< -n $(@:.cpp=) -s cpp -h h

all-local:
	$(INSTALLPROGRAMS)

clean-local:
	$(CLEANPROGRAMS)
# Tell versions [3.59,3.63) of GNU make to not export all variables.
# Otherwise a system limit (for SysV at least) may be exceeded.
.NOEXPORT:
