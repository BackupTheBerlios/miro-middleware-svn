<!DOCTYPE xml SYSTEM "PolicyEditor_behaviour.dtd">
<config> 
  <config_global name="namespace" value="Psos" />
  <config_global name="include" value="miro/SvcParameters.h" />

  <config_group name="mcp">
    <config_item parent="Miro::Tty">
      <constructor>ttyParams.baudrate = 9600;
      </constructor>
      <config_parameter name="AngleConvFactor" type="double" default="0.001534" />
      <config_parameter name="DistConvFactor" type="double" default="0.485000" />
      <config_parameter name="VelConvFactor" type="double" default="1.000000" />
      <config_parameter name="RangeConvFactor" type="double" default="1.000000" />
      <config_parameter name="DiffConvFactor" type="double" default="0.006000" />
      <config_parameter name="Vel2Divisor" type="double" default="20.000000" />
      <config_parameter name="EnableMotors" type="bool" default="false" />
    </config_item>
  </config_group>
</config>
